#include <stdlib.h>
#include <stdio.h>

int main(int argc, char* argv[])
{
	int x, cent = 0;

	if (argc != 2)
	{
		printf("Error\n");
		return (1);
	}
	else
	{
		x = atoi(argv[1]);
		if (x < 0)
		{
			printf("0\n");
		}
		else 
		{
			for (; x - 25 >= 0; x -= 25, cent++)
			{
			}
			for (; x - 10 >= 0; x -= 10, cent++)
			{
			}
			for (; x - 5 >= 0; x -= 5, cent++)
	       		{
			}
			for (; x - 2 >= 0; x -=2, cent++)
			{
			}
			if (x == 1)
			{
				cent++;
			}
			printf("%d", cent);
		}
	}
	return (0);
}
